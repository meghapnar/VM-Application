    <rule>
                
        <name>MEG_VM_VehicleStatusIcon</name>
                
        <uuid>_a-0000e37d-16a2-8000-9ba2-011c48011c48_30077</uuid>
                
        <description>Given a vehicle status, returns the appropriate icon</description>
                
        <parentUuid>_a-0000e37d-16a2-8000-9ba2-011c48011c48_29411</parentUuid>
                
        <visibility>
                        
            <advertise>false</advertise>
                        
            <hierarchy>true</hierarchy>
                        
            <indexable>true</indexable>
                        
            <quota>false</quota>
                        
            <searchable>true</searchable>
                        
            <system>false</system>
                        
            <unlogged>false</unlogged>
                    
        </visibility>
                
        <definition>#"SYSTEM_SYSRULES_iconIndicator"(
  if(ri!status,
  "FACE_HAPPY",
  "FACE_SAD") 
)</definition>
                
        <namedTypedValue>
                        
            <name>status</name>
                        
            <type>
                                
                <name>boolean</name>
                                
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
                            
            </type>
                    
        </namedTypedValue>
                
        <preferredEditor>legacy</preferredEditor>
                
        <offlineEnabled>false</offlineEnabled>
            
    </rule>
        
