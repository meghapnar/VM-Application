    <rule>
                
        <name>MEG_VM_FormatUserName</name>
                
        <uuid>_a-0000e37d-16a2-8000-9ba2-011c48011c48_29700</uuid>
                
        <description>Given a login name, returns first and last name</description>
                
        <parentUuid>_a-0000e37d-16a2-8000-9ba2-011c48011c48_29411</parentUuid>
                
        <visibility>
                        
            <advertise>false</advertise>
                        
            <hierarchy>true</hierarchy>
                        
            <indexable>true</indexable>
                        
            <quota>false</quota>
                        
            <searchable>true</searchable>
                        
            <system>false</system>
                        
            <unlogged>false</unlogged>
                    
        </visibility>
                
        <definition>user(
  ri!name,
  "firstName"
) &amp; " " &amp; user(
  ri!name,
  "lastName"
)</definition>
                
        <namedTypedValue>
                        
            <name>name</name>
                        
            <type>
                                
                <name>User</name>
                                
                <namespace>http://www.appian.com/ae/types/2009</namespace>
                            
            </type>
                    
        </namedTypedValue>
                
        <preferredEditor>legacy</preferredEditor>
                
        <offlineEnabled>false</offlineEnabled>
            
    </rule>
        
